begin bsh
bsh(  import bpp.Redirect;)
bsh()
bsh(  System.out.println("testing redirect.");)
bsh(  Redirect.begin("redirect.test1");)
bsh(  System.out.println("one");)
bsh(  Redirect.end();)
bsh(  System.out.println("finished test 1.");)
bsh(  Redirect.begin("redirect.test2");)
bsh(  System.out.println("one");)
bsh(  Redirect.end();)
bsh(  System.out.println("finished test 2, part 1");)
bsh(  Redirect.append("redirect.test2");)
bsh(  System.out.println("two");)
bsh(  Redirect.end();)
bsh(  System.out.println("finished test 2, part 2");)
bsh(  Redirect.begin("redirect.test3");)
bsh(  System.out.println("here\'s some stuff.");)
bsh(  Redirect.begin("redirect.test4");)
bsh(  System.out.println("oh, and this.");)
bsh(  Redirect.end();)
bsh(  Redirect.begin(Redirect.STDOUT);)
bsh(  System.out.println("finished test 4");)
bsh(  Redirect.end();)
bsh(  System.out.println("and some more stuff.");)
bsh(  Redirect.end();)
bsh(  System.out.println("finished test 3");)
bsh(  System.out.println("done.");)
end bsh
